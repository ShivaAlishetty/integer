#include <iostream>

using namespace std;

int main() {
    int n;

    cout << "Enter an integer: ";
    cin >> n;

    cout << "Even numbers from 0 to " << n << ":" << endl;

    for (int i = 0; i <= n; i++) {
        if (i % 2 == 0) {
            cout << i << " ";
        }
    }

    cout << endl;

    return 0;
}
